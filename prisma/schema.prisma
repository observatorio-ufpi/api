generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model RelatorioMunicipal {
  id                                              Int                                               @id @default(autoincrement())
  ano                                             DateTime                                          @db.Date
  deducoesFundebMagisterio                        DeducoesFundebMagisterio[]
  dontroleUtilizacaoRecursos                      ControleUtilizacaoRecursos[]
  deducoesParaFinsDeLimitesConstitucional         DeducoesParaFinsDeLimitesConstitucional[]
  destosAPagarInscritosDisponibilidadesFinanceira RestosAPagarInscritosDisponibilidadesFinanceira[]
  fluxoFinanceiroDeRecursos                       FluxoFinanceiroDeRecursos[]
  receita                                         Receita[]
  despesa                                         Despesa[]
}

model Receita {
  id                  Int                @id @default(autoincrement())
  tipo                ReceitaTipos
  relatorioMunicialId Int
  relatorioMunicial   RelatorioMunicipal @relation(fields: [relatorioMunicialId], references: [id])
  itensReceita        ItemReceita[]
}

model Despesa {
  id                  Int                @id @default(autoincrement())
  tipo                DespesaTipos
  relatorioMunicialId Int
  relatorioMunicial   RelatorioMunicipal @relation(fields: [relatorioMunicialId], references: [id])
  itensDespesa        ItemDespesa[]
}

model ItemReceita {
  id                           Int              @id @default(autoincrement())
  tipo                         ItemReceitaTipos
  previsaoInicial              Float
  previsaoAtualizada           Float
  receitasRealizadaBimestre    Float
  receitasRealizadaAteBimestre Float
  percentual                   Float
  topicoId                     Int
  topicosRelatorio             Receita          @relation(fields: [topicoId], references: [id])
}

model ItemDespesa {
  id                           Int              @id @default(autoincrement())
  tipo                         ItemDespesaTipos
  dotacaoInicial               Float
  dotacaoAtualizada            Float
  receitasRealizadaBimestre    Float
  receitasRealizadaAteBimestre Float
  percentual                   Float
  topicoId                     Int
  topicosRelatorio             Despesa          @relation(fields: [topicoId], references: [id])
}

model DeducoesFundebMagisterio {
  id                  Int                          @id @default(autoincrement())
  tipo                DeducoesFundebMagisterioTipo
  valor               Float
  relatorioMunicialId Int
  relatorioMunicial   RelatorioMunicipal           @relation(fields: [relatorioMunicialId], references: [id])
}

model ControleUtilizacaoRecursos {
  id                  Int                            @id @default(autoincrement())
  tipo                ControleUtilizacaoRecursosTipo
  valor               Float
  relatorioMunicialId Int
  relatorioMunicial   RelatorioMunicipal             @relation(fields: [relatorioMunicialId], references: [id])
}

model DeducoesParaFinsDeLimitesConstitucional {
  id                  Int                                         @id @default(autoincrement())
  tipo                DeducoesParaFinsDeLimitesConstitucionalTipo
  valor               Float
  relatorioMunicialId Int
  relatorioMunicial   RelatorioMunicipal                          @relation(fields: [relatorioMunicialId], references: [id])
}

model RestosAPagarInscritosDisponibilidadesFinanceira {
  id                     Int                                                 @id @default(autoincrement())
  tipo                   RestosAPagarInscritosDisponibilidadesFinanceiraTipo
  saldoAteBimestre       Float
  canceladoEmDoisMilOnze Float
  relatorioMunicialId    Int
  relatorioMunicial      RelatorioMunicipal                                  @relation(fields: [relatorioMunicialId], references: [id])
}

model FluxoFinanceiroDeRecursos {
  id                  Int                @id @default(autoincrement())
  valorFundeb         Float
  valorFundef         Float
  relatorioMunicialId Int
  relatorioMunicial   RelatorioMunicipal @relation(fields: [relatorioMunicialId], references: [id])
}

